   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "inf\n"
     ]
    }
   ],
   "source": [
    "def div(x, y):\n",
    "    return x / y\n",
    "\n",
    "\n",
    "def div_wrapper(function):\n",
    "    def div_inner(x, y):\n",
    "        if y > x: \n",
    "            x, y = y, x\n",
    "        if y == 0: return 'inf'\n",
    "        return function(x, y)\n",
    "    return div_inner\n",
    "\n",
    "div = div_wrapper(div)\n",
    "print(div(2, 0))\n",
    "            "
   ]
